    "test:run": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.2.1",
    "jsdom": "^23.0.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0"
  },
  "keywords": [
    "react",
    "ai",
    "state-management",
    "llm",
    "inference",
    "semantic-state",
    "typescript"
