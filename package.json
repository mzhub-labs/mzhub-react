{
  "name": "@mzhub/react",
  "version": "0.1.2",
  "description": "AI-Native State Management for React - The Redux for AI",
  "type": "module",
  "main": "./dist/react.cjs",
  "module": "./dist/react.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
